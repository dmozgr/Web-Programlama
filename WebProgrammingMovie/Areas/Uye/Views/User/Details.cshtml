@model WebProgrammingMovie.Models.ApplicationUser

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 style="color:white">Üye Bilgileri</h2>


<div>

    <hr />
    <div class="row">
        <div class="col-md-4">

        </div>

        <div class="col-md-4">

            <div style="color:white" class="col-sm-15">
                <h5 style="color:white"><b>Kullanıcı Fotoğrafı</b></h5><br />
                <img height="75ox" width="75px" src="@Model.PhotoURL" alt="">
                <br />
            </div>
            <div style="color:white" class="col-sm-15">
                <br />
                <h4 style="color:white"><b>İsim</b></h4>
                <h6 style="color:white">@Model.Name</h6>
                <br />
            </div>
            <div style="color:white" class="col-sm-15">
                <h4 style="color:white"><b>Soy isim</b></h4>
                <h6 style="color:white">@Model.Surname</h6>
                <br />
            </div>
            <div style="color:white" class="col-sm-15">
                <h4 style="color:white"><b>Email</b></h4>
                <h6 style="color:white">@Model.Email</h6>
                <br />
            </div>
            <div style="color:white" class="col-sm-15">
                <h4 style="color:white"><b>Doğum Tarihi</b></h4>
                <h6 style="color:white">@Model.BirthDate</h6>
                <br />
            </div>
            <div style="color:white" class="col-sm-15">
                <h4 style="color:white"><b>Ülke</b></h4>
                <h6 style="color:white">@Model.Country</h6>
                <br />
            </div>
        </div>
        <div class="col-md-4">

        </div>
    </div>
