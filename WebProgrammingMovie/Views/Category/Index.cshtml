@model IEnumerable<WebProgrammingMovie.Models.Category>

@{
    ViewData["Title"] = "Kategoriler";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Kategoriler</h1>

<table class="table">
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <h5><a asp-controller="Home" asp-action="Index" asp-route-id="@item.Id">@item.Name</a></h5>
            </td>
        </tr>
}
    </tbody>
</table>
